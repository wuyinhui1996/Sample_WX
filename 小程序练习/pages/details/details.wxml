<view class="page">
   
    <view class="page-bottom">
        <view class="userinfo">
              
                     <!-- <button wx:if="{{!hasUserInfo && canIUse}}" open-type="getUserInfo" bindgetuserinfo="getUserInfo"> 获取头像昵称 </button> -->
                     <!-- <block wx:else> -->
                     <block> 
                       <image bindtap="bindViewTap" class="userinfo-avatar" src="{{userInfo.avatarUrl}}" mode="cover"></image>
                       <text class="userinfo-nickname">{{userInfo.nickName}}</text>
                    </block>
  
        </view>
        <view class='page-item'>
          
           <button  class="wc" bindtap="CEPT_Report">
              <!-- <text>第一个item1</text> -->
              CEPT Report
            </button>
            <button  class="wc" bindtap="AT_Report">
              <!-- <text>第二个item2</text> -->
              AT Report
            </button>
            <button  class="wc" bindtap="Panel_Report">
              <!-- <text>第三个item3</text> -->
              9-Panel Report
            </button>
            <button  class="wc" bindtap="Ecg_Data">
              <!-- <text>第四个item4</text> -->
              Ecg Data
            </button>
            <button  class="wc" bindtap="Table_Data">
              <!-- <text>第五个item5</text> -->
              Table Data
            </button>
        </view>
               
    </view>
    <view  class="page-top" bindtouchmove="tap_drag" bindtouchend="tap_end" bindtouchstart="tap_start"  style="{{translate}}">
        <!-- <image bindtap="tap_ch" src="/images/more.png"></image> -->
         <!-- <view class='page-text'> -->
       <scroll-view style="height:655px;white-space:wrap" scroll-y="true" 
 scroll-into-view="{{toView}}" scroll-top="{{scrollTop}}" >   
           <!-- <view class='name'>FUWAI Hospital</view> -->
           
           <view  class='content'>
             <view class='name' style='text-align：center'>中国医学科学院阜外医院（FUwai Hospital，CAMS)</view> 
             <view class='name1' style='text-align：center;'>心肺运动实验报告总结（CPET Report)</view>
             <view class='report'style="margin-top:15px;">
               <!-- <view class='row'style="width:700rpx;height:20px;"> -->
               <!-- 横向排列：display: flex;flex-direction: row; -->
                  <view class="toLeft" style="width:700rpx;">序号（ID）：{{LIST_ID}}</view>
                  <!-- <view class="toCenter" style="width:40px;"></view> -->
                  <view class="toLeft" style="width:700rpx;">仪器设备（Instruments ID）：{{INSTRUMENTS_ID}}</view>
                  <view class="toLeft" style="width:700rpx;">姓名（Name）：{{PATIENT_NAME}}</view>
                  <view class="toLeft" style="width:700rpx;">检查日期（Date）：{{CHECK_DATE}}</view>
                  <view class="toLeft" style="width:700rpx;">病区（Inpatient Area#）：{{INPATIENT_AREA}}</view>
                  <view class="toLeft" style="width:700rpx;">出生日期（Date of Birth）：{{BIRTHDAY}}</view>
                  <view class='row' style='width:700rpx;display:flex;flex-direction:row'>
                     <view class="toCenter" style="width:500rpx;">年龄（Age）：{{AGE}}</view>
                     <view class='toRight' style='width:200rpx;'>yr.</view>
                  </view>
                   <view class="toLeft" style="width:700rpx;">病案号（Inpatient ID#）：{{INPATIENT_ID}}</view>
                    <view class='row' style='width:700rpx;display:flex;flex-direction:row'>
                     <view class="toCenter" style="width:500rpx;">性别（Gender）：{{PATIENT_SEX}}</view>
                     <view class='toRight' style='width:200rpx;'>male</view>
                  </view>
                  <view class="toLeft" style="width:700rpx;">开始时间（Strating Time）：{{START_TIME}}</view>
                  <view class='row' style='width:700rpx;display:flex;flex-direction:row'>
                     <view class="toCenter" style="width:500rpx;">身高（Height）：{{HEIGHT}}</view>
                     <view class='toRight' style='width:200rpx;'>cm</view>
                  </view>
                  <view class='row' style='width:700rpx;display:flex;flex-direction:row'>
                     <view class="toCenter" style="width:500rpx;">递增功率持续时间（Duration）：{{DURATION}}</view>
                     <view class='toRight' style='width:200rpx;'>min</view>
                  </view>
                  <view class='row' style='width:700rpx;display:flex;flex-direction:row'>
                     <view class="toCenter" style="width:500rpx;">体重（Weight）：{{WEIGHT}}</view>
                     <view class='toRight' style='width:200rpx;'>kg</view>
                  </view>
                  <view class='row' style='width:700rpx;display:flex;flex-direction:row'>
                     <view class="toCenter" style="width:500rpx;">RAMP递增方案（Protocol）：{{PROTOCOL}}</view>
                     <view class='toRight' style='width:200rpx;'>W/min</view>
                  </view>
                    <view class='row' style='width:700rpx;display:flex;flex-direction:row'>
                     <view class="toCenter" style="width:500rpx;">体质量指数（BMI）：{{BMI}}</view>
                     <view class='toRight' style='width:200rpx;'>kg/㎡</view>
                  </view>
                  <view class="toLeft" style="width:700rpx;">已知病情诊断（Diagnosis）：{{DIAGNOSIS}}</view>
                  <view class="toLeft" style="width:700rpx;">停止运动原因（Reason for Stopping）：{{RS}}</view>

                  

    <view class="table1">

      <view class="tr bg-w">
        <view class="th1 ">
          <view class='th1_ch'>指标</view>
          <view class='th1_en'>（Parameters）</view>          
        </view>
  
   
        <block wx:for="{{item1}}" wx:key="this">
          <view class='th'>{{item.parameterName}}</view>
        </block>
      </view>
      <view class='tr'>
        <view class="td ">
          <view class='td_ch'>实测值</view>
          <view class='td_en'>(Measured)</view> 
        </view>
        <view class="td ">
          <view class="tdd2">(L/min)</view>
          <view class="tdd2 border-none">(mL/min/kg)</view>
        </view>
        <view class="tdd1 ">
          <view class='tdd1_row'>
            <block wx:for="{{item1}}" wx:key="this">
             <view class='tdd1_row_num'>{{item.Measured_L}}</view>
            </block>
          </view>
    
          <view class='tdd1_row'>
           <block wx:for="{{item1}}" wx:key="this">
             <view class='tdd1_row_num'>{{item.Measured_ML}}</view>
           </block>
          </view> 
        </view>  
      </view>


      <view class='tr'>
        <view class="td  ">
          <view class='td_ch'>预计值</view>
          <view class='td_en'>（Pred.）</view> 
        </view>
        <view class="td ">
          <view class="tdd2">(L/min)</view>
          <view class="tdd2 border-none">(mL/min/kg)</view>
        </view>
        <view class="tdd1 ">
          <view class='tdd1_row'>
            <block wx:for="{{item1}}" wx:key="this">
              <view class='tdd1_row_num'>{{item.Pred_L}}</view>
            </block> 
          </view>
    
          <view class='tdd1_row'>
            <block wx:for="{{item1}}" wx:key="this">
              <view class='tdd1_row_num'>{{item.Pred_ML}}</view>
            </block>    
          </view> 
        </view>    
      </view>

      <view class='tr'>
        <view class="td  ">
          <view class='td_ch1'>百分预计值</view>     
        </view>
        <view class="td ">
          <view class="tdd">（%Pred.）</view>      
        </view>
        <view class="tdd1 ">
          <view class='tdd1_row1'>
            <block wx:for="{{item1}}" wx:key="this">
              <view class='tdd1_row_num'>{{item.Pred_Percent}}</view>
            </block>
          </view>   
        </view>
      </view>
    </view>

    
     <view class="table2">
  
  <view class="tr bg-w">
    <view class="th2 ">
      <view class='th1_ch'>指标</view>
      <view class='th1_en'>（Parameters）</view>          
    </view>
    <!-- <view class="th ">时间</view> -->
      <view class="th3 ">
        <view class='th_ch'>实测值</view>
        <view class='th_en'>(Measured)</view> 
      </view>
     <view class="th3 ">
        <view class='th_ch'>预计值</view>
        <view class='th_en'>（Pred.）</view> 
      </view>
      <view class="th3 ">
        <view class='th_ch'>百分预计值</view>
        <view class='th_en'>（%Pred.）</view> 
      </view>
  </view>

  <view class="tr bg-w" wx:for="{{item2}}"  wx:key="this">
   
      <view class='th2'>{{item.parameterName}}</view>
      <view class='th3'>{{item.measured}}</view>
      <view class='th4'>{{item.pred}}</view>
      <view class='th5'>{{item.predPercent}}</view>
  </view>    
</view>


 <view class="table3">
  <view class="tr bg-w">
    <view class="th1 ">
      <view class='th1_ch'>指标</view>
      <view class='th1_en'>（Parameters）</view>          
    </view>
   
      <view class="th ">
        <view class='th_ch'>静息</view>
        <view class='th_en'>(Resting)</view> 
      </view>
     <view class="th ">
        <view class='th_ch'>峰值</view>
        <view class='th_en'>（Peak）</view> 
      </view>
  </view>

    <view class="tr bg-w" wx:for="{{item3}}"  wx:key="this">
   
      <view class='th1'>{{item.parameterName}}</view>
      <view class='th'>{{item.resting}}</view>
      <view class='th'>{{item.peak}}</view>
    </view>

  </view>



  <view class='page_analysis'>
     <text>心肺运动数据分析解读 （CPET data Analysis Report）</text>
     <view class='page_analysis_item'>
       <view class='item_title'>整体运动能力（HolisticIntegrative Evaluationof Exercise FunctionalCapacity）:</view>
       <view class='item_text'>{{HIE}}</view>

       <view class='item_title'>运动心电图及心血管反应（Exercise ECG and CV）:</view>
       <view class='item_text'>{{ECGCV}}</view>

       <view class='item_title'>运动呼吸及流速容量环反应（Exercise Response）:</view>
       <view class='item_text'>{{ERRV}}</view>

       <view class='item_title'>静息肺功能（Resting PFT）:</view>
       <view class='item_text'>{{RESTING_PFT}}</view>

       <view class='item_name'>操作医师:{{OPE_DOCTOR}}</view>
       <view class='item_name'>判读医师:{{READ_DOCTOR}}</view>
       <view class='item_name'>审核医师:{{CHECK_DOCTOR}}</view>
     </view>
     <view class='page_analysis_blank'></view>
  </view>




                  


                  
                 
               <!-- </view> -->
             </view>
              
             
